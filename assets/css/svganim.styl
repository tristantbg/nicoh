@keyframes prev {
    0% {
        stroke-dasharray: 0%, 80%;
    }
    100% {
        stroke-dasharray: 80%, 80%;
    }
}

#prev:hover
	path
		animation prev 1.5s ease-in-out infinite

@keyframes next {
    0% {
        stroke-dasharray: 0%, 200%;
        stroke-dashoffset: 0
    }
    100% {
        stroke-dasharray: 200%, 200%;
    }
}

#next:hover
	path
		animation next 1.5s ease-in-out infinite

@keyframes start {
    0% {
        stroke-dasharray: 0%, 200%;
    }
    100% {
        stroke-dasharray: 200%, 200%;
    }
}

#start:hover
	path
		animation start 1.5s ease-in-out infinite

@keyframes texts {
    0% {
        stroke-dasharray: 0, 800%;
    }
    100% {
        stroke-dasharray: 800%, 800%;
    }
}

#texts-svg:hover
    path
        animation texts 1.5s ease-in-out infinite

@keyframes images {
    0% {
        stroke-dasharray: 0, 1000%;
    }
    100% {
        stroke-dasharray: 1000%, 1000%;
    }
}

#images-svg:hover
    path
        animation images 1.5s ease-in-out infinite

@keyframes blog {
    0% {
        stroke-dasharray: 0, 1300%;
    }
    100% {
        stroke-dasharray: 1300%, 1300%;
    }
}

#blog-svg:hover
    path
        animation blog 1.5s ease-in-out infinite  